pipeline {
    agent any
    stages {
      stage('Checkout') {
        steps {
          checkout([$class: 'GitSCM', 
                  branches: [[name: '*/master']], // Укажите нужную ветку
                  userRemoteConfigs: [[url: 'https://github.com/Vladnickgo/ProjectHotel.git']] // Укажите URL вашего репозитория
                ])
          }
        }
        stage('Scan') {
            steps {
                withSonarQubeEnv(installationName: 'sql') {
                    // Замените этот комментарий на вашу команду для запуска сканера SonarQube
                    script {
                        // Пример:
                        // bat "mvn org.sonarsource.scanner.maven:sonar-maven-plugin:3.9.0.2155:sonar"
                    }
                }
            }
        }
    }
}
